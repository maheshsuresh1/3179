{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "data": {
    "url": "data/Frog_Census_Records.csv",
    "format": {
      "type": "csv",
      "parse": {"Date": "utc:'%Y/%m/%d %H:%M:%S%Z'"}
    }
  },
  "mark": "bar",
  "encoding": {
    "x": {
      "field": "yearmonth(Date)",
      "type": "temporal",
      "axis": {"title": "Date"}
    },
    "y": {
      "aggregate": "count",
      "type": "quantitative",
      "title": "Count"
    },
    "color": {
      "field": "Type_of_observation",
      "type": "nominal",
      "title": "Type of Observation"
    }
  },
  "width": 600,
  "height": 400
}
