{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "data": {
    "url": "https://raw.githubusercontent.com/maheshsuresh1/3179/main/data/top5_uni.csv"
  },
  "transform": [
    {"filter": "datum.Field === 'Alumni'"}
  ],
  "mark": "line",
  "encoding": {
    "x": {
      "field": "Year",
      "type": "temporal",
      "axis": {"title": "Year"}
    },
    "y": {
      "field": "Value",
      "type": "quantitative",
      "axis": {"title": "Value"}
    },
    "color": {
      "field": "University",
      "type": "nominal",
      "legend": {"title": "University"}
    }
  },
  "selection": {
    "FieldSelector": {
      "type": "single",
      "fields": ["Alumni", "Award", "Hi Ci"],
      "init": "Alumni"
    }
  }
}
