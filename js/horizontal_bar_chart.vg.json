{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {
      "url": "https://raw.githubusercontent.com/maheshsuresh1/3179/main/data/data_2018.csv"
    },
    "mark": "bar",
    "encoding": {
      "y": {
        "field": "University",
        "type": "ordinal",
        "sort": "-x"
      },
      "x": {
        "field": "Total score",
        "type": "quantitative",
        "axis": {"title": "Total Score"}
      },
      "color": {
        "field": "Country",
        "type": "nominal",
        "scale": {
          "range": ["#1f77b4", "#ff7f0e", "#2ca02c", "#d62728", "#9467bd", "#8c564b", "#e377c2", "#7f7f7f", "#bcbd22", "#17becf", "#aec7e8", "#ffbb78", "#98df8a", "#ff9896", "#c5b0d5", "#c49c94", "#f7b6d2", "#dbdb8d", "#9edae5", "#c7c7c7"]
        }
      },
      "text": {
        "field": "b",
        "type": "quantitative"
      }
    }
  }
  